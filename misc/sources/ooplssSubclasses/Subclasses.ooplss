class StrictlyOrdered {
  def compare(other: MyType): Int { return 0; }
  def greater (other: MyType): Boolean {
    return self.compare(other) > 0;
  }
  def less (other: MyType): Boolean {
    return self.compare(other) < 0;
  }
}

class Ordered subclassOf StrictlyOrdered {
  def geq (other: MyType): Boolean {
    return self.compare(other) >= 0;
  }
  def leq (other: MyType): Boolean {
    return self.compare(other) <= 0;
  }
}

class Literature subclassOf Ordered {
  def __construct(author: String, year: Int) {
    self.author = author;
    self.year = year;
  }
  var author: String;
  var year: Int;
}


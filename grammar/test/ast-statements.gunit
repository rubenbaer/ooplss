gunit Ooplss;

@header {
package ch.codedump.ooplss.antlr;
}

pointExpr:
"3 * 5"     -> (* 3 5)
"3 / 5"     -> (/ 3 5)
"3 * 5 * 7" -> (* (* 3 5) 7)

dashExpr:
"3 + 5"      -> (+ 3 5)
"3 + 5 + 4"  -> (+ (+ 3 5) 4)
"3 + -5"     -> (+ 3 -5)
"3 - 5"      -> (- 3 5)
"3 * 5 + 4"  -> (+ (* 3 5) 4)
"3 * 5 + -4" -> (+ (* 3 5) -4)

inequality:
"3 <= 4"              -> (<= 3 4)
"3 <= 4 * 5"          -> (<= 3 (* 4 5))
"3 + 4 > 13 * 2 + 1"  -> (> (+ 3 4) (+ (* 13 2) 1))
"13 > 4"              -> (> 13 4)

equality:
"3 == 4"  -> (== 3 4)
"3 == 13 + 3" -> (== 3 (+ 13 3))



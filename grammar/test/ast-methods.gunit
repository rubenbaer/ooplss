gunit Ooplss;

@header {
package ch.codedump.ooplss.antlr;
}

methodDef:
<<
def foo():bar {

}
>>  -> (METHODDEF foo (RETURNTYPE bar) ARGUMENTLIST METHODBLOCK)

<<
def __construct() {

}
>> -> (METHODDEF __construct ARGUMENTLIST METHODBLOCK)

<<
def foo():bar {
	2 + 3;
	3 * 3;
}
>> -> (METHODDEF foo (RETURNTYPE bar) ARGUMENTLIST (METHODBLOCK (STMT (+ 2 3)) (STMT (* 3 3))))

<<
def foo(x:y):bar {
}
>> -> (METHODDEF foo (RETURNTYPE bar) (ARGUMENTLIST (SUBTYPEARG x y)) METHODBLOCK)

<<
def foo(x#y):bar {

}

>> -> (METHODDEF foo (RETURNTYPE bar) (ARGUMENTLIST (SUBCLASSARG x y)) METHODBLOCK)


block:
<<
{
	2 + 3;
}
>> -> (BLOCK (STMT (+ 2 3)))




gunit Ooplss;

@header {
package ch.codedump.ooplss.antlr;
}

classDec:
<<
class blah {
	def foo():bar {
		x.y().z;
	}
}
>> -> (CLASSDEF blah (METHODS (METHODDEF foo (RETURNTYPE bar) ARGUMENTLIST (METHODBLOCK (. (. (VARACCESS x) (METHODCALL y)) (MEMBERACCESS z))))))

<<
class foo {
	def method():bar {

	}

	def foo():bar {
	}

	var foo:bar;
	var a:b;
	var c:d;
}
>> ->  (CLASSDEF foo (FIELDS (VARDEF bar foo) (VARDEF b a) (VARDEF d c)) (METHODS (METHODDEF method (RETURNTYPE bar) ARGUMENTLIST METHODBLOCK) (METHODDEF foo (RETURNTYPE bar) ARGUMENTLIST METHODBLOCK)))

